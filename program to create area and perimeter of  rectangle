class rectangle
{
    int length,breadth;
    void setval(int l,int b);
    
    {
        length=l;
        breadth=b;
    }
    void area()
    {
       int ar;
       ar=length*breadth;
       System.out.println("area of rectangle: " +ar);
    }
    void perimeter()
    {
        int pr;
        pr=2*(l+b);
        System.out.println(perimeter is;" +pr);
    }
    public static void main(String args[])
    {
        rectangle r = new rectangle();
        r.setval(10,20);
        int area=r.area();
        System.out.println("area" +area);
 
        
    }
}
